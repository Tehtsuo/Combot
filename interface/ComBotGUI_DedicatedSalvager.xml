<?xml version="1.0" encoding="UTF-8"?>
<ISUI>
	<template name='Console.Font'>	
		<Name>Terminal</Name>
		<Size>8</Size>
	</template>	

	<window name='ComBot_DedicatedSalvager'>
		<Visible>1</Visible>
		<BackGroundColor></BackGroundColor>
		<Border>0</Border>
		<X>200</X>
		<Y>300</Y>
		<Width>800</Width>
		<Height>600</Height>
		<TitleBar>
			<Width>100%</Width>
			<Height>20</Height>
			<Border>0</Border>
			<BackgroundColor>FF000000</BackgroundColor>
			<Children>
				<Text name='ComBot_DedicatedSalvager_TitleBar_Title'>
					<X>0</X>
					<Y>3</Y>
					<Width>100%</Width>
					<Height>20</Height>
					<Alignment>Center</Alignment>
					<Text>Dedicated Salvager Configuration</Text>
				</Text>
				<button	Name='Close'>
					<X>r40</X>
					<Y>0</Y>
					<Width>40</Width>
					<Height>20</Height>
					<Text>Close</Text>
					<OnLeftClick>
						ui -unload "&filepath;/ComBotGUI_DedicatedSalvager"
					</OnLeftClick>
				</button>
			</Children>
		</TitleBar>
		<Children name="ComBot_DedicatedSalvager_Children">
			<Frame name="ComBot_DedicatedSalvager_Frame">
				<X>0</X>
				<Y>5</Y>
				<Width>100%</Width>
				<Height>100%</Height>
				<BackgroundColor>FF000000</BackgroundColor>
				<Border>1</Border>
				<BorderColor>FFFFFFFF</BorderColor>
				
			</Frame>
		</Children>
	</window>
	
</ISUI>
